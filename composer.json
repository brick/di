{
    "name": "brick/di",
    "description": "Dependency injection and IoC framework",
    "type": "library",
    "keywords": [
        "Brick",
        "Di",
        "IoC",
        "Dependency Injection",
        "Inversion of Control"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "brick/reflection": "~0.2.4 || ~0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "php-coveralls/php-coveralls": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Di\\Tests\\": "tests/"
        }
    }
}
