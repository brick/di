{
    "name": "brick/di",
    "description": "Dependency injection and IoC framework",
    "type": "library",
    "keywords": [
        "Brick",
        "DI",
        "IoC",
        "Dependency Injection",
        "Inversion of Control"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "brick/reflection": "~0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\DI\\Tests\\": "tests/"
        }
    }
}
